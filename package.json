{
	"name": "youtube-clickbait-filter",
	"version": "1.0.0",
	"description": "A powerful Chrome extension that intelligently filters out clickbait, negative, and crisis-related content from YouTube to improve your viewing experience.",
	"main": "src/content/filter.js",
	"type": "module",
	"keywords": [
		"chrome-extension",
		"youtube",
		"clickbait",
		"filter",
		"content-filter",
		"browser-extension",
		"manifest-v3"
	],
	"author": {
		"name": "Rory Glenn",
		"url": "https://github.com/RoryGlenn"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/RoryGlenn/YoutubeClickBaitFilter.git"
	},
	"bugs": {
		"url": "https://github.com/RoryGlenn/YoutubeClickBaitFilter/issues"
	},
	"homepage": "https://github.com/RoryGlenn/YoutubeClickBaitFilter#readme",
	"license": "MIT",
	"scripts": {
		"build": "node scripts/build.js",
		"format": "prettier --write \"**/*.{js,css,html}\"",
		"format:check": "prettier --check \"**/*.{js,css,html}\"",
		"test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
		"test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
		"test:constants": "node --experimental-vm-modules node_modules/jest/bin/jest.js constants.test.js",
		"test:performance": "node --experimental-vm-modules node_modules/jest/bin/jest.js performance.test.js",
		"test:edge-cases": "node --experimental-vm-modules node_modules/jest/bin/jest.js edge-cases.test.js",
		"test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js integration.test.js"
	},
	"private": true,
	"devDependencies": {
		"esbuild": "^0.25.8",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^30.0.5",
		"prettier": "^3.6.2"
	},
	"dependencies": {
		"convert": "^5.12.0"
	}
}
